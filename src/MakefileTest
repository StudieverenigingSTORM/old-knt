SRCDIR=src
DSTDIR=classes
BINDIR=bin
CLASSPATH=-cp ../TouchAdministration/classes:../TouchAdministrationTestImpl/classes

all: bindir dstdir classes

bindir:
	mkdir -p $(BINDIR)

dstdir:
	mkdir -p $(DSTDIR)

clean:
	test -d $(BINDIR) && rm -R $(BINDIR) || true
	test -d $(DSTDIR) && rm -R $(DSTDIR) || true

depend:
	sh updateclasses.sh MakefileTest

#BEGIN CLASSLIST

classes: $(DSTDIR)/vu/storm/touch/DemoProgram.class $(DSTDIR)/vu/storm/touch/FullscreenProgram.class $(DSTDIR)/vu/storm/touch/GUIPanel.class $(DSTDIR)/vu/storm/touch/gfx/ButtonProvider.class $(DSTDIR)/vu/storm/touch/gfx/GfxUtil.class $(DSTDIR)/vu/storm/touch/gfx/GraphicsConstants.class $(DSTDIR)/vu/storm/touch/gfx/Keypad.class $(DSTDIR)/vu/storm/touch/gfx/KeypadButton.class $(DSTDIR)/vu/storm/touch/gfx/KeypadListener.class $(DSTDIR)/vu/storm/touch/gfx/LongPressButton.class $(DSTDIR)/vu/storm/touch/gfx/Numpad.class $(DSTDIR)/vu/storm/touch/gfx/NumpadButton.class $(DSTDIR)/vu/storm/touch/gfx/NumpadListener.class $(DSTDIR)/vu/storm/touch/gfx/PageButton.class $(DSTDIR)/vu/storm/touch/gfx/StandardStyledButton.class $(DSTDIR)/vu/storm/touch/gfx/TopBar.class $(DSTDIR)/vu/storm/touch/gfx/TouchColumn.class $(DSTDIR)/vu/storm/touch/gfx/TouchRow.class $(DSTDIR)/vu/storm/touch/gfx/WordWrap.class $(DSTDIR)/vu/storm/touch/product/BorrelBar.class $(DSTDIR)/vu/storm/touch/product/LineItemButton.class $(DSTDIR)/vu/storm/touch/product/NewProductColumn.class $(DSTDIR)/vu/storm/touch/product/NumpadOverlay.class $(DSTDIR)/vu/storm/touch/product/OrderPane.class $(DSTDIR)/vu/storm/touch/product/OrderPostponeButton.class $(DSTDIR)/vu/storm/touch/product/OrderStack.class $(DSTDIR)/vu/storm/touch/product/OrderStackButton.class $(DSTDIR)/vu/storm/touch/product/OrderUserButton.class $(DSTDIR)/vu/storm/touch/product/PlaceBorrelOrderButton.class $(DSTDIR)/vu/storm/touch/product/PlaceOrderButton.class $(DSTDIR)/vu/storm/touch/product/ProductButton.class $(DSTDIR)/vu/storm/touch/product/ProductColumn.class $(DSTDIR)/vu/storm/touch/product/ProductScreen.class $(DSTDIR)/vu/storm/touch/product/ResetButton.class $(DSTDIR)/vu/storm/touch/user/UserButton.class $(DSTDIR)/vu/storm/touch/user/UserColumn.class $(DSTDIR)/vu/storm/touch/user/UserScreen.class

